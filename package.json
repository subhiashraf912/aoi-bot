{
  "name": "naze",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start": "node ./dist",
    "dev": "nodemon ./src/index.ts",
    "build": "tsc",
    "copy-canvacord-images": "copyfiles --error --up 2 src/Bot/classes/Utility/@azecord/assets/data/images/*.* dist/src/Bot",
    "copy-canvacord-fonts": "copyfiles --error --up 2 src/Bot/classes/Utility/@azecord/assets/data/fonts/*.* dist/src/Bot",
    "copy-azecord-images": "copyfiles --error --up 2 src/Bot/classes/Utility/AzeCord/aze-canvas/assets/img/*.* dist/src/Bot",
    "copy-azecord-fonts": "copyfiles --error --up 2 src/Bot/classes/Utility/AzeCord/aze-canvas/assets/fonts/*.* dist/src/Bot",
    "postbuild": "yarn copy-canvacord-images && yarn copy-canvacord-fonts && yarn copy-azecord-images && yarn copy-azecord-fonts",
    "generate-tsc": "tsc --init"
  },
  "dependencies": {
    "@discordjs/rest": "^1.3.0",
    "canvacord": "https://github.com/subhiashraf912/canvacord.git",
    "canvas": "^2.11.0",
    "discord.js": "^14.9.0",
    "gifencoder": "^2.0.1",
    "moment": "^2.29.4",
    "moment-duration-format": "^2.3.2",
    "mongoose": "^6.9.0",
    "node-canvas-with-twemoji-and-discord-emoji": "^1.2.1",
    "openai": "^3.2.1",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/gifencoder": "^2.0.1",
    "@types/moment-duration-format": "^2.2.3",
    "@types/node": "^18.11.9",
    "@types/uuid": "^9.0.1",
    "copyfiles": "^2.4.1",
    "dotenv": "^16.0.3",
    "nodemon": "^2.0.20",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4"
  }
}
