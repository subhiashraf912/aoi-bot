{
  "name": "naze",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start": "cross-env NODE_ENV=production node dist/index.js",
    "dev": "cross-env NODE_ENV=development nodemon -L --exec ts-node src/bot/index.ts | pino-pretty -c -t",
    "build": "tsc && yarn run copyfiles",
    "generate-tsc": "tsc --init",
    "copyfiles": "cpy 'src/**/*.ttf' dist"
  },
  "dependencies": {
    "@discordjs/rest": "^1.3.0",
    "canvacord": "https://github.com/subhiashraf912/canvacord.git",
    "canvas": "^2.11.0",
    "cross-env": "^7.0.3",
    "discord.js": "^14.9.0",
    "gifencoder": "^2.0.1",
    "moment": "^2.29.4",
    "moment-duration-format": "^2.3.2",
    "mongoose": "^6.9.0",
    "node-canvas-with-twemoji-and-discord-emoji": "^1.2.1",
    "openai": "^3.2.1",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/gifencoder": "^2.0.1",
    "@types/moment-duration-format": "^2.2.3",
    "@types/node": "^18.11.9",
    "@types/uuid": "^9.0.1",
    "cpy-cli": "^4.2.0",
    "dotenv": "^16.0.3",
    "nodemon": "^2.0.20",
    "pino-pretty": "^10.0.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4"
  }
}
